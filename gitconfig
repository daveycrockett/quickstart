[color]
	ui = auto
# https://wincent.com/wiki/Git_quickstart
[alias]
	st = status
	ci = commit
	co = checkout
        purr = pull --rebase
        pom = push origin master
        br = branch
	cp = cherry-pick
	unstage = reset HEAD --
	last = log -1 HEAD
[color "diff"]
	meta = cyan black
	plain = white black
	frag = yellow black
	func = magenta black
	old = red black
	new = green black
	commit = yellow black
	whitespace = black red

[core]
	autocrlf = input
        excludesfile = ~/.gitignore 
	precomposeunicode = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
